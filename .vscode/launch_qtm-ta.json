{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
            
            "--taillard",
            "--taillard-file", "/workspaces/job-shop-simulator/per_jsp/data/lawrance_instances/la20.txt",
            "--algorithm", "hybrid-tm-q",
            "--visualize",
            "--output-prefix", "la20_qtm_......q_priority",
            "--episodes=100"
            ]
        }
    ]
}

